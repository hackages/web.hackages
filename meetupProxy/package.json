{
  "name": "meetupProxy",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "watch": "port=5000 host=localhost nodemon index.js --exec babel-node --presets es2015,stage-2",
    "clean": "rm -rf ./dist",
    "lint": "eslint {index.js,libs/**/*.js,config/**/*.js}",
    "prebuild": "npm-run-all lint clean",
    "build": "babel {index.js,**/*.js} -d dist --presets es2015,stage-2",
    "i": "npm install",
    "preserve": "npm-run-all i build",
    "serve": "node dist/index.js",
    "start": "yarn serve",
    "predeploy": "yarn build",
    "deploy": "bash deploy.sh",
    "precommit": "yarn lint"
  },
  "dependencies": {
    "express": "^4.15.0",
    "express-redis-cache": "^0.5.0",
    "isomorphic-fetch": "^2.2.1",
    "npm-run-all": "^4.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "husky": "^0.13.2"
  }
}
