{
  "name": "meetupProxy",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "watch": "nodemon index.js --exec babel-node --presets es2015,stage-2",
    "prebuild": "rm -rf ./build",
    "build": "babel **/*.js -d dist --presets es2015,stage-2",
    "preserve": "yarn install",
    "serve": "node dist/index.js",
    "predeploy": "yarn build",
    "deploy": "bash deploy.sh"
  },
  "dependencies": {
    "express": "^4.15.0",
    "isomorphic-fetch": "^2.2.1",
    "rxjs": "^5.2.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0"
  }
}
